{
    "version": 4,
    "configurations": [
        {
            "name": "VX/sys x86-64",
            "intelliSenseMode": "clang-x64",
            "defines": [ "__x86_64__", "__kernel__" ],
            "includePath": [ "${workspaceFolder}/include" ],
            "compilerArgs": [
                "-ffreestanding",
                "-nostdlibinc", "-nostdlib",
                "-Wall", "-Wextra", "-pedantic"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "gnu99"
        }
    ]
}
